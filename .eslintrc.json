{
  "parser": "espree",
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": [
    "jest",
    "import",
    "node",
    "compat"
  ],
  "extends": [
    "airbnb-base",
    "plugin:import/recommended",
    "plugin:jest/recommended",
    "plugin:compat/recommended",
    "plugin:node/recommended"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "browser": true
  },
  "rules": {
    "padded-blocks": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-console": "off"
  }
}
